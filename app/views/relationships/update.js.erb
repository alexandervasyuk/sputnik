<%if @type_and_user[0] == 'ACCEPT'%>
	<%if @type_and_user[2] == 'COMPLETE'%>
		$("#friend<%@type_and_user[1].id%>").fadeOut(300, function() { $(this).remove(); })
		var friendRequests = $('#friend_requests').html();
		$('#friend_requests').html(friendRequests.valueOf() - 1)
	<% else %>
		$("#friend_form<%=@type_and_user[1].id%>").html("<%= escape_javascript(render('users/unfollow', user: @type_and_user[1])) %>")
	<% end %>
<%end%>